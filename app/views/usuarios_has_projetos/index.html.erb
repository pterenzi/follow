<h1>Listando Usuarios has projetos</h1>

<table>
  <tr>
    <th>Usuarios</th>
    <th>Projetos</th>
  </tr>

<% for usuarios_has_projeto in @usuarios_has_projetos %>
  <tr>
    <td><%=h usuarios_has_projeto.usuarios_id %></td>
    <td><%=h usuarios_has_projeto.projetos_id %></td>
    <td><%= link_to 'Mostrar', usuarios_has_projeto %></td>
    <td><%= link_to 'Alterar', edit_usuarios_has_projeto_path(usuarios_has_projeto) %></td>
    <td><%= link_to 'Excluir', usuarios_has_projeto, :confirm => 'Tem Certeza?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Cadastrar usuarios_has_projeto', new_usuarios_has_projeto_path %>
