 <% form_for(@event) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :date %>
    <%= f.date_select :date %>
 
    <%= f.label :content %>
    <%= f.text_field :content %>

  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :written_by %>
 
    <%= f.submit "Update" %>
  </p>
<% end %>

