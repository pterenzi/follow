<%= error_messages_for :usuario %>

<% form_for(@usuario, :html => { :multipart => true }) do |f| %>
   <div class="depot-form">
     <fieldset>
       <p>
         <legend>Dados do Usuario <%=  @usuario.nome  %> </legend>
         <label for="usuario_nome">Nome:</label>
         <%= f.text_field :nome, :size =>  "53" , :readonly => true %>     
       </p>
     </fieldset>
   </div> 
<% end %>

<%= link_to 'Voltar', usuarios_path %>
